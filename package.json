{
  "name": "eslint-config",
  "version": "2.0.0",
  "private": true,
  "description": "Configure eslint for sibi",
  "scripts": {
    "lint": "eslint '*.js' 'packages/**/*.js'",
    "test": "npm run lint",
    "prepare": "husky install"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sibipro/eslint-config.git"
  },
  "keywords": [],
  "author": "Sibi (https://github.com/sibipro)",
  "homepage": "https://github.com/sibipro/eslint-config",
  "license": "MIT",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "husky": "^6.0.0",
    "oao": "^2.0.1",
    "prettier": "^2.0.5"
  }
}
